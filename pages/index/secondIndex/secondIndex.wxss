.ucenter {
  background: #F6F6F6;
  width: 750rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  position: absolute;
  z-index: -2;
}

.loading-center {
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
}

.no-order {
  height: 80vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.no-order .img {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
}
.no-order .txt {
  font-size: 26rpx;
  color: #999999;
}

.order {
  margin: 5px 10px;
  display: flex;
  flex-direction: column;
}
.order .list {
  margin: 5px 0;
  background: #FFF;
  padding: 10px 5px;
  display: flex;
  flex-direction: column;
}
.order .list .top {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.order .list .top .img {
  width: 140px;
  height: 110px;
}
.order .list .top .top_info {
  padding-left: 10px;
  display: flex;
  flex-direction: column;
  font-size: 14px;
  flex-grow: 1;
}
.order .list .top .top_info .description {
  font-size: 12px;
  color: #999999;
}
.order .list .top .top_info .top_down {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.order .list .top .top_info .top_down .money_f {
  font-size: 10px;
  color: #DD3C4A;
  display: flex;
  align-items: center;
}
.order .list .top .top_info .top_down .money_f .place {
  font-size: 14px;
}
.order .list .top .top_info .top_down .money_f .money_s {
  padding-left: 10px;
  color: #999999;
  display: flex;
  align-items: center;
  text-decoration: line-through;
}
.order .list .top .top_info .top_down .money_f .money_s .original {
  font-size: 14px;
}
.order .list .top .top_info .top_down .icon {
  height: 24px;
  width: 24px;
}
.order .list .down {
  display: flex;
  justify-content: space-between;
  padding-top: 10px;
  font-size: 14px;
  color: #999999;
}
.order .list .down .down_right {
  display: flex;
  align-items: center;
}
.order .list .down .down_right .imglist {
  height: 20px;
}
.order .list .down .down_right .imglist .img {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  border: 1rpx solid #F0F0EC;
  margin-left: -10px;
  height: 20px;
  width: 20px;
}
.order .list .down .down_right .imglist-0 {
  z-index: 3;
}
.order .list .down .down_right .imglist-1 {
  z-index: 2;
}
.order .list .down .down_right .imglist-2 {
  z-index: 1;
}
.order .list .down .down_right .down_text {
  margin-left: 5px;
}
