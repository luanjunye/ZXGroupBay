.ucenter {
  background: #F6F6F6;
  width: 750rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  position: absolute;
  z-index: -2;
}

.search-bar {
  top: 0rpx;
  height: 110rpx;
  width: 100%;
  background: #F6F6F6;
}
.search-bar .search {
  margin: 0 auto;
  display: flex;
  justify-content: flex-start;
  width: 90%;
  align-items: center;
  -webkit-border-radius: 35rpx;
  -moz-border-radius: 35rpx;
  border-radius: 35rpx;
  background: #E6E6E6;
  height: 70rpx;
  padding: 0 50px;
  line-height: 70rpx;
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  font-size: 24rpx;
  color: #666;
}
.search-bar .search-logo {
  width: 27rpx;
  height: 27rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 5rpx;
}

.head {
  width: 100%;
  height: auto;
}
.head .head_info {
  width: 90%;
  height: auto;
  margin: 0 auto;
  display: flex;
  background: #F6F6F6;
  justify-content: space-around;
}
.head .head_info .head-avater {
  height: 35px;
  width: 35px;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}
.head .head_info .box {
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.head .head_info .box .name {
  font-size: 12px;
}
.head .head_info .box .address {
  font-size: 11px;
}
.head .head_info .box .img {
  width: 18px;
  height: 18px;
}
.head .head_info .box_a {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.head .head_info .box_a .name {
  font-size: 12px;
}
.head .head_info .box_a .address {
  font-size: 11px;
}
.head .head_info .box_a .img {
  width: 18px;
  height: 18px;
}

.banner_box .banner image {
  display: block;
  height: auto;
  width: 100%;
}
