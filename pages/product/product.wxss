.ucenter {
  background: #F6F6F6;
  width: 100%;
  display: flex;
  flex-direction: column;
  position: absolute;
  z-index: -2;
}

.banner_box {
  width: 750rpx;
  height: 750rpx;
}
.banner_box .banner {
  width: 100%;
  height: 100%;
}
.banner_box .banner .image {
  display: block;
  height: 100%;
  width: 100%;
}

.groupDown {
  width: 100%;
  background: #E3E4E4;
  display: flex;
}
.groupDown .groupDown_left {
  padding: 5px 20px;
  color: #FFF;
  flex-grow: 2;
  background: #0BE7C5;
}
.groupDown .groupDown_left .price .money_f {
  font-size: 14px;
  display: flex;
  align-items: center;
}
.groupDown .groupDown_left .price .money_f .place {
  font-size: 20px;
}
.groupDown .groupDown_left .price .money_f .money_s {
  padding-left: 10px;
  display: flex;
  align-items: center;
  text-decoration: line-through;
}
.groupDown .groupDown_left .price .money_f .money_s .original {
  font-size: 14px;
}
.groupDown .groupDown_left .price .money_f .remain {
  padding-left: 10px;
  font-size: 14px;
}
.groupDown .groupDown_left .bay {
  font-size: 14px;
}
.groupDown .groupDown_right {
  flex-grow: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 14px;
  color: #999999;
}

.info {
  width: 100%;
  height: 120px;
  background: #FFF;
  padding: 10px 15px;
  display: flex;
  flex-direction: column;
}
.info .info_top {
  padding-bottom: 5px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #F4F4F4;
}
.info .info_top .it_left {
  display: flex;
  flex-direction: column;
}
.info .info_top .it_left .title {
  font-size: 16px;
  color: #666666;
}
.info .info_top .it_left .description {
  margin-top: 3px;
  font-size: 14px;
  color: #999999;
}
.info .info_top .it_right {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.info .info_top .it_right .img {
  width: 14px;
  height: 14px;
}
.info .info_top .it_right .img_text {
  margin-top: 3px;
  color: #999999;
  font-size: 12px;
}
.info .info_down {
  display: flex;
  justify-content: flex-start;
  padding: 10px;
  font-size: 13px;
}
.info .info_down .id_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-right: 50px;
}
.info .info_down .id_item .ii_title {
  color: #999999;
}
.info .info_down .id_item .ii_text {
  color: #666666;
  font-size: 11px;
}

.parameter {
  margin-top: 5px;
  background: #FFF;
  padding: 10px 15px;
  color: #434343;
  font-size: 13px;
}

.activity {
  margin-top: 5px;
  background: #FFF;
  padding: 10px 15px;
  color: #434343;
  font-size: 13px;
}

.product-attr {
  background: #F6F6F6;
  width: 750rpx;
}
.product-attr .tui-nomore-box {
  padding-top: 10rpx;
}

.detail {
  background: #fff;
  width: 750rpx;
}
.detail .img {
  display: block !important;
  height: auto;
  width: 100%;
}

.like_text {
  padding: 20px 20px 0 20px;
}

.like {
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  width: 100%;
  font-size: 18px;
  padding: 10px 20px 100rpx 20px;
}
.like .likeList {
  width: 48%;
  flex-shrink: 0;
  margin-bottom: 10px;
}
.like .likeList .img {
  width: 100%;
}
.like .likeList .likeInfo {
  padding: 0 5px;
}
.like .likeList .likeInfo .likeTitle {
  font-size: 14px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.like .likeList .likeInfo .likeDescription {
  font-size: 12px;
  color: #999999;
}
.like .likeList .likeInfo .body {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}
.like .likeList .likeInfo .body .money_f {
  font-size: 10px;
  color: #DD3C4A;
  display: flex;
  align-items: center;
}
.like .likeList .likeInfo .body .money_f .place {
  font-size: 14px;
}
.like .likeList .likeInfo .body .money_f .money_s {
  padding-left: 10px;
  color: #999999;
  display: flex;
  align-items: center;
  text-decoration: line-through;
}
.like .likeList .likeInfo .body .money_f .money_s .original {
  font-size: 14px;
}
.like .likeList .likeInfo .body .icon {
  height: 24px;
  width: 24px;
}
.like .likeList .likeInfo .end {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.like .likeList .likeInfo .end .end_left {
  display: flex;
  align-items: center;
  margin-left: 10px;
}
.like .likeList .likeInfo .end .end_left .imglist {
  height: 20px;
}
.like .likeList .likeInfo .end .end_left .imglist .img {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  border: 1rpx solid #F0F0EC;
  margin-left: -10px;
  height: 20px;
  width: 20px;
}
.like .likeList .likeInfo .end .end_left .imglist-0 {
  z-index: 3;
}
.like .likeList .likeInfo .end .end_left .imglist-1 {
  z-index: 2;
}
.like .likeList .likeInfo .end .end_left .imglist-2 {
  z-index: 1;
}
.like .likeList .likeInfo .end .end_left .down_text {
  margin-left: 5px;
}
.like .likeList .likeInfo .end .end_right {
  font-size: 10px;
}
.like .likeList .likeInfo .like_down_text {
  font-size: 10px;
  text-align: end;
}

.bottom {
  position: fixed;
  bottom: 0;
  z-index: 120;
  width: 750rpx;
  height: 100rpx;
  display: flex;
  background: #fff;
  padding: 5px 20px;
  justify-content: space-between;
  align-items: center;
  border-top: 1rpx solid #EBEBEB;
}
.bottom .bottom_left {
  display: flex;
  justify-content: center;
}
.bottom .bottom_left .bl_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-right: 30px;
}
.bottom .bottom_left .bl_item .icon {
  width: 14px;
  height: 14px;
}
.bottom .bottom_left .bl_item .text {
  color: #666666;
  font-size: 12px;
}
.bottom .bottom_right {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #FFF;
}
.bottom .bottom_right .br_left {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100px;
  height: 80rpx;
  background: #0BE7C5;
  border-radius: 50px 0 0 50px;
}
.bottom .bottom_right .br_right {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  width: 100px;
  height: 80rpx;
  padding: 0 auto;
  background: #70C5A8;
  border-radius: 0 50px 50px 0;
}
